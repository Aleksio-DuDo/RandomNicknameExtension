{
  "manifest_version": 2,
  "name": "NicnamekGen",
  "version": "1.0",
  "description": "This nickname generator will end your problem with nicknames forever!",
  "icons": {
	  "128": "icon.png"
	  
  },
   "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/p5@1.1.4/lib/p5.min.js; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "NicknameGen"
  }
}